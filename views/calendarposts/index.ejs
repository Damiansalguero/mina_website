<link rel="stylesheet" href="/styles/calendar.css">
<% layout("layouts/boilerplate") %>
<h1 id="title-calendar-index" class=""><span class="line-pink">Alle Kalendereintr√§ge</span></h1>
<div class="calendar-box">
      <% for(let calendar of calendars) {%>
          <div class="list-group mb-3">
            <a href="/calendars/<%= calendar._id %>" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1"><%= calendar.title %></h5>
                <small><%= calendar.formatedDate %></small>
              </div>
              <p class="mb-1"><%- calendar.description %></p>
            </a>
        </div>
      <% }%>
    </div>
