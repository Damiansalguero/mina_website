<link rel="stylesheet" href="/styles/test.css">
<% layout("layouts/boilerplate") %>
<div class="row">
  <h1 class=""><span class="title-line line-pink">Test PDF Hinzufügen</span></h1>
  <div class="col-6 offset-3">
    <form action="/test" method="post" novalidate class="validated-form" enctype="multipart/form-data">
      <div class="mb-3">
        <label class="form-label" for="title">Überschrift</label>
        <input class="form-control" type="text" id="title" name="test[title]" required>
      </div>
      <div class="mb-3 image-input">
        <div class="form-file custom-file">
          <input class="form-file-input" id="image" type="file" name="image" multiple>
        </div>
      </div>
      <div class="mb-3">
        <button class="btn btn-success btn-mina">Speichern</button>
      </div>
    </form>
    <a class="btn btn-primary btn-mina" href="/online-bibliothek">Zurück</a>
  </div>
</div>
